{
  "$schema-template": "2.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "linyixian": {
                "username": "$CONTAINER_REGISTRY_USERNAME_linyixian",
                "password": "$CONTAINER_REGISTRY_PASSWORD_linyixian",
                "address": "linyixian.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.2",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.2",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "tmphumgps": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.tmphumgps}",
              "createOptions": {
                "NetworkingConfig": {
                  "EndpointsConfig": {
                    "host": {}
                  }
                },
                "HostConfig": {
                  "NetworkMode": "host",
                  "Privileged": true,
                  "Binds": [
                    "/dev/serial0:/dev/serial0"
                  ],
                  "Devices": [
                    {
                      "PathOnHost": "/dev/serial0",
                      "PathInContainer": "/dev/serial0",
                      "CgroupePermissions": "mrw"
                    }
                  ]
                }
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "tmphumgpsToIoTHub": "FROM /messages/modules/tmphumgps/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}